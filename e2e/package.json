{
	"name": "gramax-e2e",
	"version": "0.0.0",
	"type": "commonjs",
	"scripts": {
		"test": "cucumber-js -f junit:junit.web.xml -f progress-bar -t \"not @next-only and not @enterprise\"",
		"test:next": "cucumber-js -f junit:junit.next.xml -f progress-bar -t \"(@next or @next-only) and not @enterprise\"",
		"test:static": "cucumber-js -f junit:junit.static.xml -f progress-bar -t \"@static and not @enterprise\"",
		"test:enterprise": "cucumber-js -f junit:junit.enterprise.xml -f progress-bar -t \"@enterprise\"",
		"dev": "npm --prefix ../apps/browser run dev"
	},
	"dependencies": {
		"@cucumber/cucumber": "^10.8.0",
		"@playwright/test": "^1.56.1",
		"playwright": "^1.56.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.19.0",
		"@babel/preset-typescript": "^7.18.6",
		"ts-node": "^10.9.2"
	}
}
