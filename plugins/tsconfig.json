{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"declaration": false,
		"declarationMap": false,
		"target": "ES6", // Устанавливаем целевую версию на ES6
		"module": "ES6", // Устанавливаем модуль на ES6
		"outDir": "./dist", // Директория для выходных файлов
		"strict": true, // Включаем строгий режим (по желанию)
		"esModuleInterop": true // Включаем поддержку интероперабельности ES и CommonJS
	}
}
