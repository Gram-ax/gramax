{
	"name": "gramax-next",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build-sdk": "node ../../scripts/plugins/build-sdk.mjs",
		"build": "npm run build-sdk && next build",
		"build:standalone": "NEXT_OUTPUT_TYPE='standalone' next build",
		"start": "next start -p ${PORT}",
		"run": "npm run build && npm run start",
		"dev": "next dev -p ${PORT}",
		"lint": "next lint",
		"prune": "node-prune",
		"analyze": "cross-env ANALYZE=true next build"
	},
	"dependencies": {
		"@vitejs/plugin-react": "^4.4.1",
		"@xmldom/xmldom": "^0.9.8",
		"eslint-config-next": "^14.2.28",
		"next": "^14.2.28",
		"nookies": "^2.5.2",
		"prom-client": "^15.1.3",
		"run-parallel-limit": "^1.1.0",
		"sharp": "=0.33.5"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^14.2.28",
		"@next/eslint-plugin-next": "^14.2.28",
		"nextjs-node-loader": "^1.1.9",
		"node-loader": "^2.1.0"
	}
}
